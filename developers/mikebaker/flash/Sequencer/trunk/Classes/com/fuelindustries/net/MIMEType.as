package com.fuelindustries.net 
{

	/**
	 * @author jdolce
	 * A static class with MIMETypes as static variables.
	 * This can be used when trying to figure out what type of content is being loaded through a Loader class
	 * @see flash.net.Loader.contentType
	 */
	public class MIMEType 
	{
		public static var SWF:String = "application/x-shockwave-flash";
		public static var JPG:String = "image/jpeg";
		public static var PNG:String = "image/png";
		public static var GIF:String = "image/gif";
		
	}
}
